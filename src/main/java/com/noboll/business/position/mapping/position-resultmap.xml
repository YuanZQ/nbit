<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 

    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="position">

    <resultMap type="com.noboll.business.position.entity.Position" id="result">
    	<id property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="type" column="type"/>
        <result property="requirement" column="requirement"/>
        <result property="duty" column="duty"/>
        <result property="salaryLow" column="salary_low"/>
        <result property="salaryHigh" column="salary_high"/>
        <result property="address" column="address"/>
        
        <result property="publishTime" column="publish_time"/>
        <result property="education" column="education"/>
        <result property="workLife" column="work_life"/>
        <result property="description" column="description"/>
        <result property="num" column="num"/>
        
        <result property="deleteFlag" column="delete_flag"/>
        <result property="createUserId" column="create_user_id"/>
        <result property="createTime" column="create_time"/>
        <result property="lastModifyUserId"   column="last_modify_user_id"/>
        <result property="lastModifyTime" column="last_modify_time"/>    
        
    </resultMap>
	
</mapper>